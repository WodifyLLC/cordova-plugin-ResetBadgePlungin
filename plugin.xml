<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
        id="com.wodify.ResetBadgePlugin"
        version="1.0.0">
        
    <name>ResetBadgePlugin</name>
    
    <js-module src="www/ResetBadgePlugin.js" name="ResetBadgePlugin">
        <clobbers target="ResetBadgePlugin"/>
    </js-module>
    
    <platform name="ios">
        <source-file src="src/ios/ResetBadgePlugin.swift" framework="true" />
    </platform>
    
    <platform name="android">
        <config-file parent="/*" target="AndroidManifest.xml" />
        <source-file src="src/android/ResetBadgePlugin.java" target-dir="app/src/main/java/com/mycompany/ResetBadgePlugin" />
        <!-- If you need a library, list it here or use Gradle dependencies. -->
    </platform>
</plugin>
